<h2>Registration</h2>
<div class="row">
    <form action="/signup" method="post" id="form4" class="col s12">
        <div class="row">
            <div class="input-field col s6">
                <input placeholder="Username" name="username" type="text" class="validate">
            </div>
        </div>
        <div class="row">
            <div class="input-field col s6">
                <input placeholder="Password" name="password" type="password" class="validate">
            </div>
        </div>
        <button class="btn" form="form4" type="submit">Sing Up</button>
    </form>
</div>

